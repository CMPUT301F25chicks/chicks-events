<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/NotificationFragment">
    <fragment
        android:id="@+id/ImageAdminFragment"
        android:name="com.example.chicksevent.fragment_admin.ImageAdminFragment"
        android:label="Images Admin" >

        <action
            android:id="@+id/action_ImageAdmin_to_EventDetailFragment"
            app:destination="@id/EventDetailFragment" />

    </fragment>

    <fragment
        android:id="@+id/NotificationAdminFragment"
        android:name="com.example.chicksevent.fragment_admin.NotificationAdminFragment"
        android:label="Admin Notification Log" >
    </fragment>

    <fragment
        android:id="@+id/AdminHomeFragment"
        android:name="com.example.chicksevent.fragment_admin.AdminHomeFragment"
        android:label="Admin Home" >

        <action
            android:id="@+id/action_AdminHomeFragment_to_EventDetailFragment"
            app:destination="@id/EventDetailFragment" />

        <action
            android:id="@+id/action_adminHomeFragment_to_NotificationFragment"
            app:destination="@id/NotificationFragment" />

        <action
            android:id="@+id/action_adminHome_to_eventAdminFragment"
            app:destination="@id/EventAdminFragment"/>
        <action
            android:id="@+id/action_adminHome_to_orgAdminFragment"
            app:destination="@id/OrgAdminFragment"/>
        <action
            android:id="@+id/action_adminHome_to_profileAdminFragment"
            app:destination="@id/ProfileAdminFragment"/>

        <action
            android:id="@+id/action_adminHome_to_imageAdminFragment"
            app:destination="@id/ImageAdminFragment"/>



        <action
            android:id="@+id/action_adminHome_to_notificationAdminFragment"
            app:destination="@id/NotificationAdminFragment"/>


        <action
            android:id="@+id/action_AdminHomeFragment_to_EventFragment"
            app:destination="@id/EventFragment"/>

        <action
            android:id="@+id/action_AdminHomeFragment_to_CreateEventFragment"
            app:destination="@id/CreateEventFragment"/>

        <action
            android:id="@+id/action_AdminHomeFragment_to_ProfileFragment"
            app:destination="@id/ProfileFragment"/>

    </fragment>



    <fragment
        android:id="@+id/EventAdminFragment"
        android:name="com.example.chicksevent.fragment_admin.EventAdminFragment"
        android:label="Events Admin">

        <action
            android:id="@+id/action_EventAdminFragment_to_EventDetailFragment"
            app:destination="@id/EventDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/OrgAdminFragment"
        android:name="com.example.chicksevent.fragment_admin.OrgAdminFragment"
        android:label="Organizers Admin"/>
    <fragment
        android:id="@+id/ProfileAdminFragment"
        android:name="com.example.chicksevent.fragment_admin.ProfileAdminFragment"
        android:label="Profiles Admin"/>

    <fragment
        android:id="@+id/NotificationFragment"
        android:name="com.example.chicksevent.fragment.NotificationFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_notification">

        <action
            android:id="@+id/action_NotificationFragment_to_AdminHomeFragment"
            app:destination="@id/AdminHomeFragment" />

        <action
            android:id="@+id/action_NotificationFragment_to_EventFragment"
            app:destination="@id/EventFragment" />

        <action
            android:id="@+id/action_NotificationFragment_to_NotificationFragment"
            app:destination="@id/NotificationFragment" />

        <action
            android:id="@+id/action_NotificationFragment_to_CreateEventFragment"
            app:destination="@id/CreateEventFragment" />

        <action
            android:id="@+id/action_NotificationFragment_to_EventDetailFragment"
            app:destination="@id/EventDetailFragment" />

        <action
            android:id="@+id/action_NotificationFragment_to_ProfileFragment"
            app:destination="@id/ProfileFragment" />
    </fragment>

    <fragment
        android:id="@+id/QRCodeScannerFragment"
        android:name="com.example.chicksevent.fragment.QRCodeScannerFragment"
        android:label="Scan QR Code"
        tools:layout="@layout/fragment_qr_code_scanner">

        <action
            android:id="@+id/action_QRCodeScannerFragment_to_NotificationFragment"
            app:destination="@id/NotificationFragment" />
        <action
            android:id="@+id/action_QRCodeScannerFragment_to_EventFragment"
            app:destination="@id/EventFragment" />
        <action
            android:id="@+id/action_QRCodeScannerFragment_to_EventDetailFragment"
            app:destination="@id/EventDetailFragment">
            <argument
                android:name="eventName"
                app:argType="string" />
        </action>
        <action
            android:id="@+id/action_QRCodeScannerFragment_to_CreateEventFragment"
            app:destination="@id/CreateEventFragment" />
        <action
            android:id="@+id/action_QRCodeScannerFragment_to_ProfileFragment"
            app:destination="@id/ProfileFragment" />
    </fragment>

    <fragment
        android:id="@+id/QRCodeDisplayFragment"
        android:name="com.example.chicksevent.fragment.QRCodeDisplayFragment"
        android:label="@string/event_detail_fragment_label"
        tools:layout="@layout/fragment_event_detail">


        <action
            android:id="@+id/action_QRCodeDisplayFragment_to_QRCodeScannerFragment"
            app:destination="@id/QRCodeScannerFragment" />
    </fragment>
    <fragment
        android:id="@+id/EventDetailFragment"
        android:name="com.example.chicksevent.fragment.EventDetailFragment"
        android:label="@string/event_detail_fragment_label"
        tools:layout="@layout/fragment_event_detail">

        <action
            android:id="@+id/action_EventDetailFragment_to_QRCodeScannerFragment"
            app:destination="@id/QRCodeScannerFragment" />

