        android:name="com.example.chicksevent.fragment.WaitingListFragment"
        android:label="@string/waiting_list_fragment_label"
        tools:layout="@layout/fragment_waiting_list">

        <action
            android:id="@+id/action_WaitingListFragment_to_EventDetailFragment"
            app:destination="@id/EventDetailFragment" />
        <action
