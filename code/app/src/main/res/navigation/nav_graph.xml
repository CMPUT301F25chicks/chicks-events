        <action
            android:id="@+id/action_EventDetailFragment_to_HostedEventFragment"
            app:destination="@id/HostedEventFragment" />
        <action
            android:id="@+id/action_EventDetailFragment_to_WaitingListFragment"
            app:destination="@id/WaitingListFragment" />
        <action
            android:id="@+id/action_EventDetailFragment_to_ChosenListFragment"
            app:destination="@id/ChosenListFragment" />
        <action
            android:id="@+id/action_EventDetailFragment_to_NotificationFragment"
            app:destination="@id/NotificationFragment" />
        <action
            android:id="@+id/action_EventDetailFragment_to_EventFragment"
            app:destination="@id/EventFragment" />
        <action
            android:id="@+id/action_EventDetailFragment_to_CreateEventFragment"
            app:destination="@id/CreateEventFragment" />
    </fragment>

    <fragment
        android:id="@+id/CreateEventFragment"
        android:name="com.example.chicksevent.fragment.CreateEventFragment"
        android:label="Create Event"
        tools:layout="@layout/fragment_create_event">

        <action
            android:id="@+id/action_CreateEventFragment_to_EventFragment"
            app:destination="@id/EventFragment" />

        <action
            android:id="@+id/action_CreateEventFragment_to_NotificationFragment"
            app:destination="@id/NotificationFragment" />
        <action
            android:id="@+id/action_CreateEventFragment_to_ProfileFragment"
            app:destination="@id/ProfileFragment" />


        </fragment>

    <fragment
        android:id="@+id/EventFragment"
        android:name="com.example.chicksevent.fragment.EventFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_event">

        <action
            android:id="@+id/action_EventFragment_to_NotificationFragment"
            app:destination="@id/NotificationFragment" />

        <action
            android:id="@+id/action_EventFragment_to_EventDetailFragment"
            app:destination="@id/EventDetailFragment" />

        <action
            android:id="@+id/action_EventFragment_to_CreateEventFragment"
            app:destination="@id/CreateEventFragment" />
        <action
            android:id="@+id/action_EventFragment_to_SearchEventFragment"
            app:destination="@id/SearchEventFragment" />
        <action
            android:id="@+id/action_EventFragment_to_HostedEventFragment"
            app:destination="@id/HostedEventFragment" />
        <action
            android:id="@+id/action_EventFragment_to_ProfileFragment"
            app:destination="@id/ProfileFragment" />
    </fragment>

    <fragment
        android:id="@+id/EntrantLocationMapFragment"
        android:name="com.example.chicksevent.fragment_org.EntrantLocationMapFragment"
        android:label="Entrant Locations Map"
        tools:layout="@layout/fragment_entrant_location_map">

    </fragment>

    <fragment
        android:id="@+id/ProfileFragment"
        android:name="com.example.chicksevent.fragment.ProfileFragment"
        android:label="@string/profile_fragment_label"
        tools:layout="@layout/fragment_profile_entrant">

        <action
            android:id="@+id/action_ProfileFragment_to_NotificationFragment"
            app:destination="@id/NotificationFragment" />
        <action
            android:id="@+id/action_ProfileFragment_to_CreateEventFragment"
            app:destination="@id/CreateEventFragment" />
        <action
            android:id="@+id/action_ProfileFragment_to_EventFragment"
            app:destination="@id/EventFragment" />

    </fragment>

    <fragment
        android:id="@+id/SearchEventFragment"
        android:name="com.example.chicksevent.fragment.SearchEventFragment"
        android:label="@string/search_event_fragment_label"
        tools:layout="@layout/fragment_search_event">

        <action
            android:id="@+id/action_SearchEventFragment_to_HostedEventFragment"
            app:destination="@id/HostedEventFragment" />
        <action
            android:id="@+id/action_SearchEventFragment_to_EventFragment"
            app:destination="@id/EventFragment" />
        <action
            android:id="@+id/action_SearchEventFragment_to_NotificationFragment"
            app:destination="@id/NotificationFragment" />
        <action
            android:id="@+id/action_SearchEventFragment_to_CreateEventFragment"
            app:destination="@id/CreateEventFragment" />
        <action
            android:id="@+id/action_SearchEventFragment_to_EventDetailFragment"
            app:destination="@id/EventDetailFragment" />
    </fragment>

    <fragment
        android:id="@+id/HostedEventFragment"
        android:name="com.example.chicksevent.fragment_org.HostedEventFragment"
        android:label="@string/hosted_event_fragment_label"
        tools:layout="@layout/fragment_hosted_event">

        <action
            android:id="@+id/action_HostedEventFragment_to_SearchEventFragment"
            app:destination="@id/SearchEventFragment" />
        <action
            android:id="@+id/action_HostedEventFragment_to_EventFragment"
            app:destination="@id/EventFragment" />
        <action
            android:id="@+id/action_HostedEventFragment_to_UpdateEventFragment"
            app:destination="@id/UpdateEventFragment" />
        <action
            android:id="@+id/action_HostedEventFragment_to_EventDetailOrgFragment"
            app:destination="@id/EventDetailOrgFragment" />
        <action
            android:id="@+id/action_HostedEventFragment_to_NotificationFragment"
            app:destination="@id/NotificationFragment" />
        <action
            android:id="@+id/action_HostedEventFragment_to_CreateEventFragment"
            app:destination="@id/CreateEventFragment" />
    </fragment>

    <fragment
        android:id="@+id/EventDetailOrgFragment"
        android:name="com.example.chicksevent.fragment_org.EventDetailOrgFragment"
        android:label="@string/event_detail_org_fragment_label"
        tools:layout="@layout/fragment_update_event_detail">

    <action
        android:id="@+id/action_HostedEventFragment_to_EventDetailOrgFragment"
        app:destination="@id/EventDetailOrgFragment" />

        <action
            android:id="@+id/action_EventDetailOrgFragment_to_EntrantLocationMapFragment"
            app:destination="@id/EntrantLocationMapFragment" />

        <action
            android:id="@+id/action_EventDetailOrgFragment_to_WaitingListFragment"
            app:destination="@id/WaitingListFragment" />
        <action
            android:id="@+id/action_EventDetailOrgFragment_to_ChosenListFragment"
            app:destination="@id/ChosenListFragment" />
        <action
            android:id="@+id/action_EventDetailOrgFragment_to_CancelledListFragment"
            app:destination="@id/CancelledListFragment" />
        <action
