        <action
            android:id="@+id/action_WaitingListFragment_to_NotificationFragment"
            app:destination="@id/NotificationFragment" />
        <action
            android:id="@+id/action_WaitingListFragment_to_EventFragment"
            app:destination="@id/EventFragment" />
        <action
            android:id="@+id/action_WaitingListFragment_to_CreateEventFragment"
            app:destination="@id/CreateEventFragment" />
    </fragment>

